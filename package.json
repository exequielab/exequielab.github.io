{
  "name": "vite-svelte-tailwindcss",
  "version": "0.0.0",
  "scripts": {
    "data": "js-yaml public/data.yml > public/data.json",
    "dev": "vite",
    "prebuild": "npm run data",
    "build": "vite build",
    "postbuild": "echo \"remove slash in index.html for css and js\"",
    "serve": "vite preview",
    "fix": "eslint --fix \"{,!(node_modules|public|docs|build|dist)/**/}*.{js,svelte}\"",
    "format": "prettier --write \"{,!(node_modules|public|docs|build|dist)/**/}*.{js,svelte}\"",
    "prepare": "husky install",
    "deploy": "gh-pages -d build -r https://github.com/exequielab/exequielab.github.io.git -b master"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.30",
    "@tsconfig/svelte": "^2.0.1",
    "@types/three": "^0.143.0",
    "autoprefixer": "^10.3.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.22.0",
    "eslint-plugin-svelte3": "^3.1.2",
    "gh-pages": "^3.1.0",
    "husky": "^5.2.0",
    "js-yaml": "^3.14.0",
    "postcss": "^8.3.6",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.7",
    "svelte-preprocess": "^4.9.8",
    "tailwindcss": "^3.0.23",
    "three": "^0.143.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "vite": "^2.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": "npm run fix && npm run format"
  }
}
